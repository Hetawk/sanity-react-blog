{
    "buildCommand": "craco build && cd backend_api && npm install --production && npx prisma generate",
    "devCommand": "craco start",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/*.js": {
            "maxDuration": 10,
            "includeFiles": "backend_api/{routes,utils,prisma,node_modules}/**"
        }
    },
    "build": {
        "env": {
            "DISABLE_ESLINT_PLUGIN": "true",
            "SKIP_PREFLIGHT_CHECK": "true",
            "FAST_REFRESH": "false",
            "GENERATE_SOURCEMAP": "false"
        }
    }
}