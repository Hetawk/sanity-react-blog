{
    "buildCommand": "npm install && cd server && npm install && cd .. && craco build",
    "devCommand": "craco start",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
        "api/index.js": {
            "maxDuration": 10,
            "includeFiles": "server/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "build": {
        "env": {
            "DISABLE_ESLINT_PLUGIN": "true",
            "SKIP_PREFLIGHT_CHECK": "true",
            "FAST_REFRESH": "false",
            "GENERATE_SOURCEMAP": "false"
        }
    }
}