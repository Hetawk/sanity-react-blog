{
    "buildCommand": "craco build && cp -r backend_api/routes api/ && cp -r backend_api/utils api/ && cp -r backend_api/prisma api/ && cp backend_api/server.js api/",
    "devCommand": "craco start",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "build": {
        "env": {
            "DISABLE_ESLINT_PLUGIN": "true",
            "SKIP_PREFLIGHT_CHECK": "true",
            "FAST_REFRESH": "false",
            "GENERATE_SOURCEMAP": "false"
        }
    }
}