{
    "buildCommand": "npm install && craco build && cp -r server/* api/ && cd api && npm install",
    "devCommand": "craco start",
    "installCommand": "npm install",
    "framework": null,
    "functions": {
        "api/index.js": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "build": {
        "env": {
            "DISABLE_ESLINT_PLUGIN": "true",
            "SKIP_PREFLIGHT_CHECK": "true",
            "FAST_REFRESH": "false",
            "GENERATE_SOURCEMAP": "false"
        }
    }
}